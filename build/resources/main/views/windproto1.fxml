<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane focusTraversable="true" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ctrl.EMRController">
    <top>
        <VBox prefHeight="91.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <MenuBar>
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Close" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Delete" />
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="More...">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About" />
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <TabPane prefHeight="72.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab text="Editor">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="36.0" prefWidth="600.0">
                                    <children>
                                        <Button layoutX="65.0" layoutY="6.0" mnemonicParsing="false" prefHeight="16.0" prefWidth="26.0" text="+" />
                                        <Label layoutX="14.0" layoutY="10.0" text="Option :" />
                                        <Label layoutX="138.0" layoutY="10.0" text="Modes :" />
                                        <Button layoutX="99.0" layoutY="6.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="26.0" text="Button" />
                                        <Button layoutX="189.0" layoutY="6.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="29.0" text="Button" />
                                        <Button layoutX="223.0" layoutY="6.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="27.0" text="Button" />
                                        <Button layoutX="256.0" layoutY="6.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="30.0" text="Button" />
                                        <Button layoutX="292.0" layoutY="6.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="28.0" text="Button" />
                                        <ComboBox layoutX="328.0" layoutY="6.0" prefHeight="25.0" prefWidth="79.0" promptText="Arrows" />
                                        <Button layoutX="413.0" layoutY="6.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="31.0" text="Button" />
                                        <Label layoutX="460.0" layoutY="10.0" text="Aligment :" />
                                        <Button layoutX="523.0" layoutY="6.0" mnemonicParsing="false" prefHeight="19.0" prefWidth="29.0" text="Button" />
                                        <Button layoutX="558.0" layoutY="6.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="27.0" text="Button" />
                                        <Label layoutX="601.0" layoutY="6.0" />
                                        <Label layoutX="601.0" layoutY="10.0" text="Inversion :" />
                                        <Button layoutX="668.0" layoutY="6.0" mnemonicParsing="false" prefHeight="21.0" prefWidth="25.0" text="Button" />
                                    </children></AnchorPane>
                            </content>
                        </Tab>
                        <Tab text="Simulator">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">

                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </VBox>
    </top>
    <left>
        <Accordion fx:id="accordion" BorderPane.alignment="CENTER">
            <panes>
                <TitledPane animated="false" onMouseClicked="#onTitledPaneClicked" text="Energy-based models">
                    <content>
                  <FlowPane fx:id="energyBasedPane" />
                    </content>
                </TitledPane>
                <TitledPane animated="false" onMouseClicked="#onTitledPaneClicked" text="Inversion-based models">
                    <content>
                        <FlowPane fx:id="inversionBasedPane">
                     <children>
                        <Canvas fx:id="inversionAccumulationCanva" height="200.0" width="200.0" />
                        <Canvas fx:id="inversionConversionCanva" height="200.0" width="200.0" />
                        <Canvas fx:id="inversionCouplingCanva" height="200.0" width="200.0" />
                     </children></FlowPane>
                    </content>
                </TitledPane>
                <TitledPane animated="false" onMouseClicked="#onTitledPaneClicked" text="Strategy models">
                    <content>
                        <FlowPane fx:id="strategyBasedPane" />
                    </content>
                </TitledPane>
                <TitledPane animated="false" onMouseClicked="#onTitledPaneClicked" text="Estimator models">
                    <content>
                        <FlowPane fx:id="estimatorBasedPane" />
                    </content>
                </TitledPane>
            </panes>
        </Accordion>
    </left>
    <bottom>
        <Label prefHeight="1.0" prefWidth="734.0" text="Label" textAlignment="CENTER" BorderPane.alignment="CENTER" />
    </bottom>
    <right>
        <ScrollBar orientation="VERTICAL" BorderPane.alignment="CENTER" />
    </right>
    <center>
        <VBox alignment="BOTTOM_LEFT" focusTraversable="true" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <Canvas fx:id="canva" height="392.0" onMouseClicked="#onCanvaClicked"  width="498.0" />
                <ScrollBar prefHeight="14.0" prefWidth="430.0" />
            </children></VBox>
    </center>

</BorderPane>
